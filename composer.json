{
    "name": "yandinovriandi/laravel-cloudflare-api",
    "type": "library",
    "description": "Laravel API for Cloudflare",
    "minimum-stability": "beta",
    "keywords": [
        "cloudflare",
        "laravel",
        "rest-api"
    ],
    "homepage": "https://mikrotikbot.com",
    "license": "MIT",
    "authors": [
        {
            "name": "yandinovriandi",
            "email": "yandinovriandi@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": ">=8.0",
        "guzzlehttp/guzzle": ">=7.0",
        "guzzlehttp/psr7": ">=2.0",
        "php": ">=8.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "yandinovriandi\\Cloudflare\\Providers\\CloudflareServiceProvider"
            ],
            "aliases": {
                "Cloudflare": "yandinovriandi\\Cloudflare\\Facades\\Cloudflare"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "yandinovriandi\\Cloudflare\\": "src"
        }
    }
}